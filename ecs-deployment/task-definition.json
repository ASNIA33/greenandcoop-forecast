            "essential": true,
            "environment": [
                {
                    "name": "MONGO_URI",
                    "value": "mongodb+srv://mbodjabdselam33:uKOipSBHr7AbpYmD@forecast-cluster.meeiptz.mongodb.net/?appName=forecast-cluster"
